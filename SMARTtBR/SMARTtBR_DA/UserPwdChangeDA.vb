Imports SMARTtBR_DALImports SMARTtBR_BOImports SMARTtBR_COPublic Class UserPwdChangeDA    Private M_DBConn As Connection    Private M_UPID As Integer    Public Function Save_Data(ByVal M_UserPwdChange As UserPwdChangeBO, ByVal M_EntryMode As String, ByVal M_MenuID As Integer, ByVal UserTime As String) As Integer        Try            Dim M_Sql As String            With M_UserPwdChange                M_Sql = "Exec IUM_UserPwdChange_Sp "                M_Sql = M_Sql & "'" & M_EntryMode & "',"                M_Sql = M_Sql & M_MenuID & ","                M_Sql = M_Sql & .UPID & ","                M_Sql = M_Sql & .UPUMID & ","                M_Sql = M_Sql & "'" & .UPCurrPwd & "',"                M_Sql = M_Sql & "'" & .UPNewPwd & "',"                M_Sql = M_Sql & .MakerID & ","                M_Sql = M_Sql & "'" & UserTime & "'"            End With            M_UPID = M_DBConn.ExecuteScalar(M_Sql)            Return M_UPID        Catch ex As Exception            MsgBox(ex.Message)            Return False        End Try    End Function    Public Function Locate_Data(ByVal M_UPID As String) As UserPwdChangeBO        Try            Dim M_Dt As New DataTable            Dim M_UserPwdChange As New UserPwdChangeBO            Dim Qry As String            Qry = "Select * from UserPwdChange_M_Tbl where UP_ID = " & M_UPID            M_DBConn.ExecuteDataTable(M_Dt, Qry)            If M_Dt.Rows.Count > 0 Then                With M_UserPwdChange                    .UPUMID = M_Dt.Rows(0).Item("UP_UMID")                    .MakerID = M_Dt.Rows(0).Item("Maker_ID")                    .MakingTime = M_Dt.Rows(0).Item("Making_Time")                    .UPID = M_Dt.Rows(0).Item("UP_ID")                    .UPCurrPwd = M_Dt.Rows(0).Item("UP_CurrPwd")                    .UPNewPwd = M_Dt.Rows(0).Item("UP_NewPwd")                End With            End If            Return M_UserPwdChange        Catch ex As Exception            Return Nothing        End Try    End Function    Public Sub New()        Try            M_DBConn = New Connection        Catch ex As Exception        End Try    End Sub    Protected Overrides Sub Finalize()        Try            M_DBConn = Nothing            MyBase.Finalize()        Catch ex As Exception        End Try    End SubEnd Class