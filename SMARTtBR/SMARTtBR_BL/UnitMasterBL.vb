Imports SMARTtBR_DAImports SMARTtBR_BOImports SMARTtBR_BL.CommonBLImports SMARTtBR_CO.CommonClassPublic Class UnitMasterBL    Private M_UNID As Integer    Public Function Save_Data(ByVal M_UnitMaster As UnitMasterBO, ByVal M_EntryMode As String, ByVal M_MenuID As Integer) As Integer        Try            Dim M_UnitMasterDA As New UnitMasterDA            Dim UserTime As String            If CheckUserRight(M_EntryMode, M_UnitMaster.MakerID, M_MenuID) = True Then                UserTime = Get_UserTime()                M_UNID = M_UnitMasterDA.Save_Data(M_UnitMaster, M_EntryMode, M_MenuID, UserTime)                M_UnitMasterDA = Nothing                Return M_UNID            Else                Return 0            End If        Catch ex As Exception            Return 0        End Try    End Function    Public Function Locate_Data(ByVal M_UNID As Integer) As UnitMasterBO        Try            Dim M_UnitMasterDA As New UnitMasterDA            Return M_UnitMasterDA.Locate_Data(M_UNID)            M_UnitMasterDA = Nothing        Catch ex As Exception            Return Nothing        End Try    End Function    Public Function Check_UnitNameExists(ByVal M_CmpCode As Integer, ByVal M_UnitName As String, ByVal M_UNID As Long) As Boolean        Try            Dim M_Return As Boolean            Dim M_UnitMasterDA As New UnitMasterDA            M_Return = M_UnitMasterDA.Check_UnitNameExists(M_CmpCode, M_UnitName, M_UNID)            M_UnitMasterDA = Nothing            Return M_Return        Catch ex As Exception            Return Nothing        End Try    End FunctionEnd Class