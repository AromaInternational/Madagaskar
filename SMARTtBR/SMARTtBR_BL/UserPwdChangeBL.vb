Imports SMARTtBR_DAImports SMARTtBR_BOImports SMARTtBR_BL.CommonBLImports SMARTtBR_CO.CommonClassPublic Class UserPwdChangeBL    Private M_UPID As Integer    Public Function Save_Data(ByVal M_UserPwdChange As UserPwdChangeBO, ByVal M_EntryMode As String, ByVal M_MenuID As Integer) As Integer        Try            Dim M_UserPwdChangeDA As New UserPwdChangeDA            Dim UserTime As String            If CheckUserRight(M_EntryMode, M_UserPwdChange.MakerID, M_MenuID) = True Then                UserTime = Get_UserTime()                M_UPID = M_UserPwdChangeDA.Save_Data(M_UserPwdChange, M_EntryMode, M_MenuID, UserTime)                M_UserPwdChangeDA = Nothing                Return M_UPID            Else                Return 0            End If        Catch ex As Exception            Return 0        End Try    End Function    Public Function Locate_Data(ByVal M_UPID As Integer) As UserPwdChangeBO        Try            Dim M_UserPwdChangeDA As New UserPwdChangeDA            Return M_UserPwdChangeDA.Locate_Data(M_UPID)            M_UserPwdChangeDA = Nothing        Catch ex As Exception            Return Nothing        End Try    End FunctionEnd Class